<!DOCTYPE html>
<html>
<head>
  <title>Gundam Model Kits - Gunpla</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "jquery-ui-1.8.14.custom.min" %>
  <%= javascript_include_tag "jquery.Jcrop.js" %>
  <%= javascript_include_tag "jquery.easing.1.3.js" %>
  <%= javascript_include_tag "jquery.mCustomScrollbar.js" %>
  <%= javascript_include_tag "jquery.mousewheel.min.js" %>
  <%= stylesheet_link_tag("jquery-ui-1.8.14.custom") %>
  <%= stylesheet_link_tag("jquery.Jcrop.css") %>
  <%= stylesheet_link_tag("jquery.mCustomScrollbar.css") %>
  <%= csrf_meta_tag %>
</head>
<body id="store">
	<div id="banner">
		<%= @page_title || "Gunpla List" %>
	</div>
	<div id="columns">
		<div id="side">
			<ul>
				<li><%= link_to "Elenco completo", gunplas_path %></li>
				<li><%= link_to "Importa da CSV", cosmicimport_path %></li>
				<li><%= link_to "Esporta ", export_path %></li>
				<li><%= link_to "Gestione Categorie ", categories_path %></li>
				<li><%= link_to "Ricerca Immagini ", googleimage_path %></li>
			</ul>
		</div>
		<div id="main">
			<%= yield %>
		</div>
	</div>
</body>
</html>
